{
  "logger": {
    "filename": "access.log",
    "format": "[<level>{level: <6}</level>] <green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> - <cyan>{name}</cyan>:<cyan>{function}</cyan> - <level>{message}</level>",
    "level": "info",
    "path": "log/access.log",
    "retention": "1 months",
    "rotation": "5 days"
  }
}
